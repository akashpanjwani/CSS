{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sp-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spPage/SpPageWebPart.js",
          "manifest": "./src/webparts/spPage/SpPageWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.4.1.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
      "globalName": "bootstrap"
    },
    "carousel": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.min.js",
      "globalName": "carousel"
    }
  },
  "localizedResources": {
    "SpPageWebPartStrings": "lib/webparts/spPage/loc/{locale}.js"
  }
}